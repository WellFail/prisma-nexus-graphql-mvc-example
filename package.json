{
  "name": "typescript-nexus-prisma-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "generate": "yarn -s generate:prisma && yarn -s generate:nexus",
    "generate:prisma": "npx prisma2 generate",
    "generate:nexus": "GENERATE_NEXUS=true NODE_ENV=development ts-node --transpile-only src/graphql/schema",
    "migrate": "prisma2 migrate save --experimental --create-db --name='auto' && prisma2 migrate up --experimental --auto-approve",
    "migrate:up": "prisma2 migrate up --experimental --create-db --auto-approve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prisma/cli": "^2.1.3",
    "@types/node": "^13.7.6",
    "eslint": "^6.7.2",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.50",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@nexus/schema": "^0.14.0",
    "@prisma/client": "^2.1.3",
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "apollo-server": "^2.15.1",
    "bcrypt": "^5.0.0",
    "cuid": "^2.1.8",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "inversify": "^5.0.1",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13"
  }
}
